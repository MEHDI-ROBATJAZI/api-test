    body
        h1.m-3 this is test rest api with one example
        .container
            .input-group.mb-5(id="form")
                label(for="username") username
                input.form-control(type="text",v-model="userName")
                label(for="email") email
                input.form-control(type="email",v-model="email")
                label(for="password") password
                input.form-control(type="password",v-model="password")
                button.btn.btn-primary(v-on:click="signUp") signUp

            

            table.table.table-striped.table-bordered(id="table")
                thead
                    tr
                        th userName
                        th email
                        th password
                        th signUp_Date
                tbody
                    tr(v-for="user in people")
                        td {{user.userName}}
                        td {{user.email}}
                        td {{user.password}}
                        td {{user.signUpDate}}


        script(src="/js/script.js")